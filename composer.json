{
    "name": "ricardosierra/rss",
    "description": "RSS builder for Laravel 5",
    "keywords": ["rss", "laravel", "laravel5"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Sierra",
            "email": "contato@ricardosierra.com.br"
        },
        {
            "name": "thujohn",
            "email": "jonathan.thuau@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "4.*|5.*|6.*|7.*"
    },
    "autoload": {
        "psr-4": {
            "RicardoSierra\\Rss\\": "src/Rss/"
        }
    },
    "autoload-dev": {
      "psr-4": {
          "RicardoSierra\\Test\\": "tests"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RicardoSierra\\Rss\\RssServiceProvider"
            ],
            "aliases": {
                "Rss": "RicardoSierra\\Rss\\RssFacade"
            }
        }
    }

}
